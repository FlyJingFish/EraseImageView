<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EraseView">
        <attr name="FlyJFish_erase_resource" format="reference" />
        <attr name="FlyJFish_erase_DrawPathType" format="enum">
            <enum name="Circle" value="0" />
            <enum name="Read" value="1" />
            <enum name="Serpentine" value="2" />
            <enum name="Lightning" value="3" />
        </attr>
        <attr name="FlyJFish_erase_paddingLeft" format="dimension" />
        <attr name="FlyJFish_erase_paddingTop" format="dimension" />
        <attr name="FlyJFish_erase_paddingRight" format="dimension" />
        <attr name="FlyJFish_erase_paddingBottom" format="dimension" />
        <attr name="FlyJFish_erase_paddingStart" format="dimension" />
        <attr name="FlyJFish_erase_paddingEnd" format="dimension" />

        <attr name="FlyJFish_erase_radius" format="dimension" />

        <attr name="FlyJFish_erase_resource_percentLeft" format="float" />
        <attr name="FlyJFish_erase_resource_percentTop" format="float" />
        <attr name="FlyJFish_erase_resource_percentRight" format="float" />
        <attr name="FlyJFish_erase_resource_percentBottom" format="float" />

        <attr name="FlyJFish_erase_duration" format="integer" />
        <attr name="FlyJFish_erase_repeatCount" format="integer" />
        <attr name="FlyJFish_erase_repeatMode" format="enum">
            <enum name="restart" value="1" />
            <enum name="reverse" value="2" />
        </attr>
        <attr name="FlyJFish_erase_autoStart" format="boolean" />
        <attr name="FlyJFish_erase_eraseMode" format="boolean" />
        <attr name="FlyJFish_erase_handMode" format="boolean" />
    </declare-styleable>
</resources>